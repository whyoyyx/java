	</setting>
	
	<setting type="about_url">
		<des>关于</des>
		<value>http://aisenyuedu.duapp.com/?page_id=18</value>
	</setting>
	
	<setting type="help_url">
		<des>帮助</des>
		<value>http://aisenyuedu.duapp.com/?p=31</value>
	</setting>
	
</app-settings>